<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>

  <appSettings>
    <!--<add key="connectionString" value="uNAhvwuwoSzikT303y+QFyR2SWvd2Jz/GUR6qj+8JXuI2+Z0No58zJUuiiCObGu3Z7gSdokXsBWnG64mjWvsSdzto2YfpZVp"  />-->
    <add key="SecurityKey" value="ETrainSecurityKey"/>
    <!--Tự động làm mới dữ liệu theo giay -->
    <add key="TimeDelay" value="30"/>
    <add key="IsDebugMode" value="1"/>
    
  </appSettings>
  <connectionStrings>
    <add name="ETrains.Properties.Settings.dbTrainConnectionString"
      connectionString="Data Source=THANHTUNG-PC;Initial Catalog=dbTrain;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>

  <log4net debug="false">
    <appender name="LogFileAppender" type="log4net.Appender.FileAppender,log4net">
      <param name="File" value="Logs\\EcustomsLog.txt"/>
      <param name="AppendToFile" value="true"/>
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%d [%t] %-5p %c [%x] &lt;%X{auth}&gt; - %m%n"/>
      </layout>
    </appender>
    <root>
      <level value="Error"></level>
      <priority value="1"/>
      <appender-ref ref="LogFileAppender"/>
    </root>
  </log4net>
<startup><supportedRuntime version="v2.0.50727"/></startup></configuration>
